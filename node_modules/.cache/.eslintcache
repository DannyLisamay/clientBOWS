essed = event => {\r\n        if (event.keyCode === 13) {\r\n            ResortListFromApi();\r\n        }\r\n    };\r\n    return (\r\n        <div>\r\n            <div className=\"aboutme\">\r\n                <p className=\"homepage\">\r\n                    Please enter a resort you want to search for down below!\r\n                </p>\r\n            </div>\r\n            <div className=\"background\">\r\n                <div className=\"searchbar\">\r\n\r\n                    <input type=\"text\" id=\"searchResortInput\" className=\"searchbar\" placeholder=\"Please enter your zipcode or a Resort name\" onKeyDown={enterPressed}></input>\r\n\r\n                    <button onClick={ResortListFromApi} className=\"searchbutton\">SUBMIT </button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default Home;","/workspace/BOWS/client/src/components/Navbar/index.js",[],"/workspace/BOWS/client/src/components/Navbar/Navbar.js",[],"/workspace/BOWS/client/src/pages/mountainfinder.js",[],"/workspace/BOWS/client/src/components/Footer/Footer.js",[],"/workspace/BOWS/client/src/components/Resort/Resort.js",["89","90","91"],"/workspace/BOWS/client/src/components/Resort/data/weatherAPI.js",[],"/workspace/BOWS/client/src/components/Resort/WeatherWidget.js",["92"],"// Resort component \nimport React, { useEffect, useState } from 'react'\n// Bootstrap\nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport { Container, Row } from 'react-bootstrap';\nimport { getWeatherData } from './data/weatherAPI'\n// days array\nconst days = [\"Sunday\", \"Monday\", \"Tuesday\", \"Wednesday\", \"Thursday\", \"Friday\", \"Saturday\"];\n// date \nconst date = new Date();\n// current day\nconst day1 = days[date.getDay()];\n// 2 days after current day\nconst day2 = days[(date.getDay() + 1) % 7];\nconst day3 = days[(date.getDay() + 2) % 7];\n\nconst WeatherWidget = (city) => {\n    // weather Data\n    // list data is unix UTC\n    const [weatherdata, se